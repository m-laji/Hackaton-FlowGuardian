    "X_train['Timestamp'] = (X_train['Timestamp'] - pd.Timestamp(\"1970-01-01\")) // pd.Timedelta('1D')\n",
    "\n",
    "X_test['Timestamp'] = pd.to_datetime(X_test['Timestamp'])\n",
    "X_test['Timestamp'] = (X_test['Timestamp'] - pd.Timestamp(\"1970-01-01\")) // pd.Timedelta('1D')"
