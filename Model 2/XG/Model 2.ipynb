        "import pandas as pd",
        "from sklearn.model_selection import train_test_split",
        "from sklearn.metrics import classification_report",
        "from xgboost import XGBClassifier",
        "from sklearn.preprocessing import StandardScaler",
        "import joblib"
      ]
    }
